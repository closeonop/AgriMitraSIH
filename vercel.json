{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_OPENWEATHER_API_KEY": "@openweather_api_key",
    "VITE_AGROMONITORING_API_KEY": "@agromonitoring_api_key",
    "VITE_AMBEE_API_KEY": "@ambee_api_key",
    "VITE_OPENCAGE_API_KEY": "@opencage_api_key"
  }
}